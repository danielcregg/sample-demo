{
	"name": "Java",
	"image": "mcr.microsoft.com/vscode/devcontainers/base:ubuntu",
	"features": {
		"desktop-lite": "latest",
		"java": "lts"
	},
	"forwardPorts": [6080],
	"portsAttributes": {
		"6080": {
			"label": "VNC",
			"onAutoForward": "openBrowser"
		}
	},
	"postCreateCommand": ./bin/bash sdk i java 17.0.1.fx-zulu;sdk default java 17.0.1.fx-zulu
}
